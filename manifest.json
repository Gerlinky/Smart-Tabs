{
  "manifest_version": 1,

  "name": "Smart Tabs",
  "description": "Creating a smarter way to use tabs to avoid overload",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
