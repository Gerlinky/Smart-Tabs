{
  "manifest_version": 2,

  "name": "Smart Tabs",
  "description": "Creating a smarter way to use tabs to avoid overload",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  
  ],
  "background": {
    "scripts": ["background.js"]
  }
  

}
